/* pages/luntan/fabiao.wxss */
/* 上传的图片 */
.img-list {
    display: flex;
    flex-wrap: wrap;
  }
  .img-li {
    width: 200rpx;
    height: 200rpx;
    margin-right: 39rpx;
    margin-bottom: 23rpx;
  }
   
  .img-li:first-child {
    margin-right: 0;
  }
  .img-li image {
    width: 40%;
    height: 40%;
  }
  .icon-delete {
    width: 28rpx !important;
    height: 28rpx !important;
    position: relative;
    float: right;
    margin-top: -229rpx;
    margin-right: -15rpx;
    z-index: 99;
  }
  .icon-deletes {
    width: 28rpx !important;
    height: 28rpx !important;
    position: relative;
    float: right;
    margin-top: -9rpx;
    margin-left: -10rpx;
    margin-right: 29rpx;
    z-index: 99;
  }
  .content-input-z {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 24rpx;
    color: #999999;
  }
   
  .content-input-z {
    margin-top: 31rpx;
  }
   
  .content-input-z view image {
    width: 32rpx;
    height: 31rpx;
    margin-right: 11rpx;
  }
  .content-input-z view {
    display: flex;
    align-items: center;
  }








  .text-box {
    width: 100%;
    position: relative;
    min-height: 80rpx;
    margin-top: 17rpx;
    }
    
    .text-box text {
    display: block;
    visibility: hidden;/*元素不可见*/
    word-break: break-all;
    word-wrap: break-word;
    }
    
    .text-box .weui-textarea {
    height: 100%;
    width:96%;
    position: absolute;
    left: 10rpx;
    top: 0;
    overflow-y: hidden;
    word-break: break-all;/*换行*/
    word-wrap: break-word;/*换行*/
    border: 1rpx solid rgb(100, 97, 97);
    }
