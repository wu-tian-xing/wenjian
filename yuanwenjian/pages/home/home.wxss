.banner{
  width:100%;
}
.autoImg
{
  width:100%;
}

page{
  height:100%;
}

.header{
 
  border-left-width: 3px;
  border-left-style: solid;
  border-left-color: rgb(10, 134, 10);
display: flex;
  justify-content: space-between;
  align-content: center;
  height: 45rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  margin-top: 10rpx;
 
}
.text-all{
  color: rgb(236, 68, 96);
  font-size: 15px;
  padding-top:6rpx;
}

.navs {
  padding-top: 60rpx;
  display: flex;
}
.nav-item {
 width: 300rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  /* padding: 20rpx; */
}
.nav-item .nav-image {
  width: 65rpx;
  height: 65rpx;
  /* margin: 5rpx; */
}
.nav-item text {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: gray;
}

.imagesize-6
{   display:flex;
    justify-content:center;

    width:66%;
    height:600rpx;
    top:10rpx;
}
.size-6
{top:10rpx;
height:100%;
width:100%;
left:0rpx;
}
.btn-6[plain]
{

top:10rpx;
left:0rpx;
border:none;
}

.imagesize-7
{   display:flex;
    justify-content:center;

    width:66%;
    height:600rpx;
    top:10rpx;
}
.size-7
{top:10rpx;
height:95%;
width:100%;
left:800rpx;
}
.btn-7[plain]
{

top:-400rpx;
left:0rpx;
border:none;
}


  




